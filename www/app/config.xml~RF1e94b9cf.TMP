<!--  // from CP files ///   -->
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" source="npm"/>   <!-- the add on info source npm from S Northcoftt lesson -->
<!--  Whitelists  -->
    <access origin="*" />
    <access origin="" browserOnly="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
<!--    <allow-intent href="tel:*" />-->
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
 
     
<!-- ICONS & SPLASH SCREENS -->      
  	
  	<icon src="icons/Icon-small@2x.png" />		<!-- default icon for when don't know what icon to use -->
  	<gap:splash src="splashScrns/mdpi.png" />	<!-- default splash screen -->

   <!-- ANDROID -->
    <platform name="android">
        <allow-intent href="market:*" />
        
      	<icon src="icons/ldpi.png" gap:platform="android" gap:qualifier="ldpi" />
      	<icon src="icons/mdpi.png" gap:platform="android" gap:qualifier="mdpi" />
      	<icon src="icons/hdpi.png" gap:platform="android" gap:qualifier="hdpi" />
      	<icon src="icons/xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />
      	<icon src="icons/xxhdpi.png" gap:platform="android" gap:qualifier="xxhdpi" />
      	
		<gap:splash src="splashScrns/ldpi.png" gap:qualifier="ldpi" />
     	<gap:splash src="splashScrns/mdpi.png" gap:qualifier="mdpi" />
     	<gap:splash src="splashScrns/hdpi.png" gap:qualifier="hdpi" />
     	<gap:splash src="splashScrns/xhdpi.png" gap:qualifier="xhdpi" />
     	<gap:splash src="splashScrns/xxhdpi.png" gap:qualifier="xxhdpi" />
      	
    </platform>
 
	<!-- iOS -->
   

    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
   
    	<!-- iPhone iPhone / iPod Touch -->  
    	<icon src="icons/Icon-60.png" gap:platform="ios" width="60" height="60" />
   		<icon src="icons/Icon-60@2x.png" gap:platform="ios" width="120" height="120" />
   		
   		<gap:splash src="splashScrns/Default.png" gap:platform="ios" width="320" height="480" />
   		<gap:splash src="splashScrns/Default@2x.png" gap:platform="ios" width="640" height="960" />
   		
   		<!-- iPhone 5 / iPod Touch (5th Generation) -->
   		<gap:splash src="splashScrns/Default-568h@2x.png" gap:platform="ios" width="640" height="1136" />
   		
   		
   		<!-- iPhone 6 / 6+ -->
   		
   		<icon src="icons/Icon-60@3x.png" gap:platform="ios" width="180" height="180" />
   		
   		<!-- iPhone 6 -->
   		<gap:splash src="splashScrns/Default-667h@2x.png" gap:platform="ios" width="750" height="1334" />
   		<gap:splash src="splashScrns/Default-Portrait-736@2x.png" gap:platform="ios" width="1242" height="2208" />

   		
    	<!-- iPad -->
   		<icon src="icons/Icon-76.png" gap:platform="ios" width="76" height="76" />    	
   		<icon src="icons/Icon-76@2x.png" gap:platform="ios" width="152" height="152" />
   	    
   	    <gap:splash src="splashScrns/Default-Portrait.png" gap:platform="ios" width="768" height="1024" />
   	    <gap:splash src="splashScrns/Default-Portrait@2x.png" gap:platform="ios" width="1536" height="2048" />    	
    	    	
		<!-- Settings Icon -->    	    	
   		<icon src="icons/Icon-small.png" gap:platform="ios" width="29" height="29" /> 
   		<icon src="icons/Icon-small@2x.png" gap:platform="ios" width="58" height="58" />    				
		

        <!-- Spotlight Icon -->
   		<icon src="icons/Icon-40.png" gap:platform="ios" width="40" height="40" />
   		<icon src="icons/Icon-40@2x.png" gap:platform="ios" width="80" height="80" />  
   
   
